@model OnboardingManagement.Models.Project
<div>@Html.Partial("_HeadNavBar")</div>
<div>@Html.Partial("_Toolbar")</div>
@using (Html.BeginForm("Create", "Project", FormMethod.Post))
{
    <center>
        <div class="form-group">

            <h3 style="color:darkblue ;font-style:oblique"> Create Project</h3><br/>
            <br/>
            <div class="col-lg-6 col-lg-offset-3" style="background-color:lemonchiffon ; border:groove;" >

                <div class="row">
                    <div class="col-lg-3">
                        <br/><br/><br /><br />


                        @Html.Label("Project: Name")
                    </div>
                    <div class="col-lg-3">
                        <br /><br /><br /><br />
                        @Html.Kendo().TextBoxFor(m => Model.P_Name)


                    </div>
                </div><br />

                <!--End of first row-->
                <div class="row">
                    <div class="col-lg-3">
                        @Html.Label("Project Technology:")
                    </div>
                    <div class="col-lg-3">
                        @Html.Kendo().TextBoxFor(m => Model.P_Technology)
                    </div>
                </div><br />
                <!--End of second row-->

                <div class="row">
                    <div class="col-lg-3">

                    </div>
                    <div class="col-lg-3">
                        <button class="btn btn-lg btn-primary" type="submit">Create</button>
                        
                    </div>
                </div><br />
            </div>

        </div>
    </center>
}




