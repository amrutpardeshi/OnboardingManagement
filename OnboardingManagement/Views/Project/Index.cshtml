


<br/><br/>



@(Html.Kendo().Grid<OnboardingManagement.Models.Project>()
      .Name("grid")
      .Columns(columns =>
      {
          columns.Bound(c => c.P_Name).Title("Project Name");
          columns.Bound(c => c.P_Technology).Title("Technology");
          
         
      })
      .Scrollable()
      .Pageable()
      .DataSource(dataSource => dataSource
          .Ajax()
           .Model(model => model.Id(p => p.P_Id))
          .Read(read => read.Action("Projects_Read", "Project"))
           

      )
     
     
)


